---
layout: none
title: Team
permalink: api/staff.json
---
[
{% for member in site.data.staff %}
  {
    "name": {{ member['name'] | jsonify }},
    "email": {{ member['email'] | jsonify }},
    "role": {{ member['role'] | jsonify }},
    "preferred_title": {{ member['preferred_title'] | jsonify }},
    "what_you_want_to_be_when_you_grow_up": {{ member['what_you_want_to_be_when_you_grow_up'] | jsonify }},
    "catch_phrase": {{ member['catch_phrase'] | jsonify }},
    "image": {{ member['image'] | prepend: site.baseurl | jsonify }},
    "bio": {{ member['bio_html'] | jsonify }}"
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]